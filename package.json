{
  "name": "dave.io",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "bun run build && wrangler pages dev ./dist",
    "test:unit": "vitest",
    "test:e2e": "nightwatch tests/e2e/*",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint:oxlint": "oxlint . --fix -D correctness --ignore-path .gitignore",
    "lint:eslint": "eslint . --fix",
    "lint": "run-s lint:*",
    "format": "prettier --write src/",
    "deploy": "bun run build && wrangler pages deploy ./dist",
    "switch-branch": "rm -rf dist node_modules bun.lockb .trunk; git checkout main; bun install"
  },
  "dependencies": {
    "pinia": "^2.2.8",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20241127.0",
    "@nightwatch/vue": "^3.1.2",
    "@tsconfig/node22": "^22.0.0",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.10.1",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vitejs/plugin-vue-jsx": "^4.1.1",
    "@vitest/eslint-plugin": "1.1.13",
    "@vue/eslint-config-prettier": "^10.1.0",
    "@vue/eslint-config-typescript": "^14.1.4",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "autoprefixer": "^10.4.20",
    "chromedriver": "^131.0.1",
    "eslint": "^9.16.0",
    "eslint-plugin-oxlint": "^0.14.0",
    "eslint-plugin-vue": "^9.32.0",
    "geckodriver": "^5.0.0",
    "jsdom": "^25.0.1",
    "nightwatch": "^3.9.0",
    "npm-run-all2": "^7.0.1",
    "oxlint": "^0.14.0",
    "postcss": "^8.4.49",
    "prettier": "^3.4.1",
    "tailwindcss": "^3.4.15",
    "ts-node": "^10.9.2",
    "typescript": "~5.6.3",
    "vite": "^6.0.1",
    "vite-plugin-nightwatch": "^0.4.6",
    "vite-plugin-vue-devtools": "^7.6.7",
    "vitest": "^2.1.6",
    "vue-tsc": "^2.1.10",
    "wrangler": "^3.91.0"
  },
  "trustedDependencies": [
    "@nightwatch/nightwatch-inspector",
    "chromedriver",
    "esbuild",
    "geckodriver",
    "vue-demi",
    "workerd"
  ],
  "packageManager": "pnpm@9.13.2+sha512.88c9c3864450350e65a33587ab801acf946d7c814ed1134da4a924f6df5a2120fd36b46aab68f7cd1d413149112d53c7db3a4136624cfd00ff1846a0c6cef48a"
}
